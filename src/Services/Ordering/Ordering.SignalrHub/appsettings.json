{
  "spring": {
    "cloud": {
      "config": {
        "name": "ordering.signalrhub",
        "uri": "http://localhost:8888",
        "validateCertificates": false,
        "retry": {
          "enabled": true
        }
      }
    }
  },
  "eureka": {
    "instance": {
      "appName": "orderingsignalrhub",
      "metadataMap": {
        "externalUrl": "${vcap:application:uris:0?http://localhost:5202}"
      }
    }
  }
}