{
  "Logging": {
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Warning",
      "System": "Warning",
      "Microsoft": "Warning",
      "Steeltoe": "Warning"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldRegisterWithEureka": true,
      "shouldFetchRegistry": true
    },
    "instance": {
      "metadataMap": {
        "externalUrl": "${vcap:application:uris:0?externalUrl}"
      }
    }
  },
  "management": {
    "endpoints": {
      "path": "/cloudfoundryapplication"
    }
  }
}