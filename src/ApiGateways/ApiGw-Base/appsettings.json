{
  "spring": {
    "cloud": {
      "config": {
        "uri": "http://localhost:8888",
        "validateCertificates": false,
        "retry": {
          "enabled": true
        }
      }
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "shouldFetchRegistry": true
    },
    "instance": {
      "metadataMap": {
        "externalUrl": "${vcap:application:uris:0?externalUrl}"
      }
    }
  }
}