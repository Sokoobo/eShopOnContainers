{
  "urls": {
    "basket": "http://basket.api",
    "catalog": "http://catalog.api",
    "orders": "http://ordering.api"
  },
  "spring": {
    "application": {
      "name": "webshoppingagg"
    }
  },
  "eureka": {
    "instance": {
      "appName": "webshoppingagg",
      "metadataMap": {
        "externalUrl": "${vcap:application:uris:0?http://localhost:5121}"
      }
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "management": {
    "endpoints": {
      "path": "/cloudfoundryapplication"
    }
  }
}
